def comparar_datas(d1, m1, a1, d2, m2, a2):
    if a1 == a2 and m1 == m2 and d1 == d2:
        return 0
        
    if a1 == a2:
        if m1 == m2:
            if d1 < d2:
                return -1
            else:
                return 1
        elif m1 < m2:
            return -1
        else:
            return 1
    elif a1 < a2:
        return -1
    else:
        return 1
    
